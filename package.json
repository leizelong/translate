{
  "name": "translate",
  "displayName": "translate",
  "description": "多语言翻译",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.41.0"
  },
  "publisher": "leizl",
  "categories": [
    "Other"
  ],
  "icon": "images/logo.png",
  "activationEvents": [
    "onCommand:extension.translate-i18n",
    "onCommand:extension.translate-auto"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leizelong/translate.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.translate-i18n",
        "title": "翻译多语言",
        "category": "translate-i18n"
      },
      {
        "command": "extension.translate-auto",
        "title": "自动翻译多语言",
        "category": "translate-auto"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.7.1",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "@leizl/google-translate-open-api": "0.0.1",
    "json5": "^2.1.1",
    "lodash": "^4.17.15",
    "md5": "^2.2.1"
  }
}
