{
  "name": "translate",
  "displayName": "translate",
  "description": "多语言翻译",
  "version": "0.0.17",
  "license": "MIT",
  "engines": {
    "vscode": "^1.41.0"
  },
  "publisher": "leizl",
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:extension.translate-i18n",
    "onCommand:extension.translate-auto"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leizelong/translate.git"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.translate-i18n",
        "title": "翻译多语言",
        "category": "translate-i18n"
      },
      {
        "command": "extension.translate-auto",
        "title": "自动翻译多语言",
        "category": "translate-auto"
      }
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "prepare": "husky install",
    "version:alpha": "standard-version --prerelease alpha",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/glob": "^7.1.1",
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.7.1",
    "@types/vscode": "^1.41.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "glob": "^7.1.5",
    "husky": "^5.1.3",
    "mocha": "^6.2.2",
    "standard-version": "^9.1.1",
    "ts-loader": "^8.0.17",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@leizl/google-translate-open-api": "^0.0.2",
    "json5": "^2.1.1",
    "lodash": "^4.17.15",
    "md5": "^2.2.1"
  }
}
